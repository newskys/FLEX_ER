{
    "name": "FLEXER",
    "manifest_version": 3,
    "version": "1.6",
    "icons": {
        "32": "flxr-32.png",
        "64": "flxr-64.png",
        "128": "flxr-128.png"
      },
    "description": "the simple extension for Flex",
    "action": {
        "default_title": "FLEXER",
        "default_popup": "popup.html"
    },
    "permissions": [
        "scripting",
        "storage",
        "sidePanel"
    ],
    "host_permissions": [
        "https://flex.team/",
        "https://flexer-server.vercel.app/*"
    ],
    "background": {
        "service_worker": "dist/service-worker.js"
    }
}
